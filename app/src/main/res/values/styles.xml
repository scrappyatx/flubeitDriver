<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">

        <!-- colorPrimary ==> toolbar background -->
        <item name="colorPrimary">@color/colorPrimary</item>

        <!-- colorPrimaryDark ==> status bar & navigation bar background -->
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>

        <!-- colorAccent ==> Bright complement to colorPrimary.  Applied to controls -->
        <item name="colorAccent">@color/colorAccent</item>

        <!-- colorControlNormal ==> The color applied to framework controls in their normal state. -->
        <item name="colorControlNormal">@color/colorAccent</item>

        <!-- colorControlActivated ==> The color applied to framework controls in their activated state -->
        <item name="colorControlActivated">@color/colorAccent</item>

        <!-- colorControlHighlight ==> The color applied to framework control highlights (ex. ripples, list selectors). -->
        <item name="colorControlHighlight">@color/colorAccent</item>

        <!-- colorButtonNormal ==> The color applied to framework buttons in their normal state. -->
        <item name="colorButtonNormal">@color/colorAccent</item>

        <!-- colorSwitchThumbNormal ==> The color applied to framework switch thumbs in their normal state. (switch off) -->
        <item name="colorSwitchThumbNormal">@color/colorAccent</item>

        <item name="android:actionBarStyle">@style/ToolBarStyle</item>
    </style>

    <style name="AppTheme.Fullscreen" parent="AppTheme">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <!-- ToolBar -->
    <style name="ToolBarStyle" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <item name = "android:textAppearance">@style/TextAppearance.AppCompat.Widget.ActionBar.Title</item>
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:textColorSecondary">@android:color/white</item>
        <item name="actionMenuTextColor">@android:color/white</item>
        <item name="android:background">@color/colorPrimaryDark</item>
    </style>

    <!-- Overflow button -->
    <style name="ToolbarOverflowButtonStyle" parent="Base.Theme.AppCompat.Light.DarkActionBar">

    </style>

    <!-- Switch Color (TextSize here affects CAPTION text to left of switch )-->
    <style name = "SwitchStyleColor">
        <item name = "colorControlActivated">@color/colorAccent</item>
        <item name = "colorSwitchThumbNormal">@color/md_grey_300</item>
        <item name = "android:colorForeground">@color/md_grey_600</item>
        <item name = "android:textSize">16sp</item>
        <item name = "android:textColor">@color/colorPrimary</item>
    </style>

    <!--- Switch Text (TextSize here affects text inside switch -->
    <style name = "SwitchStyleText" parent="TextAppearance.AppCompat.Widget.Switch">
